{
	"name": "njs-nest-pdfconsultor-backend",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint --ext .ts ./src",
		"lint:fix": "npm run lint -- --fix",
		"vi:test": "vitest",
		"vi:cov": "vitest run --coverage --watch",
		"coverage": "vitest run --coverage",
		"test": "vitest run",
		"test:run": "vitest --run --watch",
		"prepare": "husky install",
		"commit:cz": "yarn cz",
		"sonar": "yarn vi:cov && yarn eslint:report && node sonarqube-scanner.ts",
		"eslint:report": "npx eslint -f json -o eslint-report.json --ext .ts ./src",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"test:e2e:watch": "jest --config ./test/jest-e2e.json --watch"
	},
	"dependencies": {
		"@google-cloud/logging-winston": "^6.0.0",
		"@nestjs-addons/in-memory-db": "^3.0.3",
		"@nestjs/axios": "^3.0.1",
		"@nestjs/common": "^9.1.6",
		"@nestjs/config": "^3.1.1",
		"@nestjs/core": "^9.1.6",
		"@nestjs/mapped-types": "^2.0.2",
		"@nestjs/platform-express": "^9.4.3",
		"@nestjs/swagger": "^7.1.14",
		"@nestjs/terminus": "^10.1.1",
		"@nestjs/typeorm": "^10.0.0",
		"@supercharge/request-ip": "^1.2.0",
		"@validatecl/rut": "^4.0.3",
		"axios": "^1.6.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"joi": "^17.11.0",
		"nest-winston": "^1.9.4",
		"pdf-to-base64": "^1.0.3",
		"pg": "^8.8.0",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^5.0.5",
		"rxjs": "^7.8.1",
		"swagger-ui-express": "^5.0.0",
		"typeorm": "^0.3.17",
		"winston": "^3.11.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.2.0",
		"@commitlint/config-conventional": "^18.1.0",
		"@faker-js/faker": "^8.2.0",
		"@nestjs/cli": "^10.2.1",
		"@nestjs/schematics": "^10.0.2",
		"@nestjs/testing": "^9.1.6",
		"@types/dotenv": "^8.2.0",
		"@types/express": "^4.17.20",
		"@types/jest": "^29.5.7",
		"@types/node": "^20.8.9",
		"@types/supertest": "^2.0.15",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"@vitest/coverage-istanbul": "^0.34.6",
		"cz-conventional-changelog": "3.3.0",
		"eslint": "^8.52.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.29.0",
		"eslint-plugin-prettier": "^5.0.1",
		"husky": "^8.0.3",
		"jest": "^29.7.0",
		"lint-staged": "^15.0.2",
		"lodash": "^4.17.21",
		"moment": "^2.29.4",
		"node-irr": "^2.0.5",
		"node-notifier": "^10.0.1",
		"prettier": "^3.0.3",
		"sonarqube-scanner": "^3.1.0",
		"supertest": "^6.3.3",
		"ts-jest": "^29.0.3",
		"ts-loader": "^9.5.0",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.2.2",
		"vite-tsconfig-paths": "^3.6.0",
		"vitest": "^0.34.6",
		"vitest-sonar-reporter": "^0.5.0",
		"xml2js": "^0.6.2"
	},
	"lint-staged": {
		"**.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --config ./.prettierrc.js --write"
		],
		"**/*.{css,scss,md,html,json}": [
			"prettier --config ./.prettierrc.js --write"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
